<script>
  // Select all dropdown elements
  const mobilehotlines = document.querySelectorAll(".official-hotlines-mobile ");

  mobilehotlines.forEach((dropdown) => {
    // Select the numberSubmenu for the current dropdown
    const numberSubmenu = dropdown.querySelector(".drop-the-hotline-numbers");

    // Toggle the numberSubmenu when clicking the dropdown
    dropdown.addEventListener("click", function (event) {
      event.stopPropagation(); // Prevents click from affecting other elements

      // Close all other numberSubmenus
      mobilehotlines.forEach((otherDropdown) => {
        if (otherDropdown !== dropdown) {
          const othernumberSubmenu = otherDropdown.querySelector(".drop-the-hotline-numbers");
          othernumberSubmenu.classList.remove("show"); // Hide other numberSubmenus
        }
      });

      // Toggle the current numberSubmenu
      numberSubmenu.classList.toggle("show"); // Toggles the 'show' class
    });
  });

  // Hide the numberSubmenu when clicking outside any dropdown
  document.addEventListener("click", function (event) {
    mobilehotlines.forEach((dropdown) => {
      const numberSubmenu = dropdown.querySelector(".drop-the-hotline-numbers");
      if (!dropdown.contains(event.target)) {
        numberSubmenu.classList.remove("show");
      }
    });
  });
</script>



