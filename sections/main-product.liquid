<script>
  class ProductCustomDescriptionLoader {
    static load() {
        let productDescription = document.querySelector(".product-description");
        let customDescriptions = document.querySelectorAll(".product-custom-description");
        let customDescriptionCount = customDescriptions.length;

        for (let index = 0; index < customDescriptionCount; index++) {
            let customDescription = customDescriptions[index];
            let content = productDescription.querySelector(`#${customDescription.getAttribute("data-content")}`);
            content.remove();
            content.classList.remove("custom-description-content");

            customDescription.append(content);
        }
    }
}
  ProductCustomDescriptionLoader.load();
</script>
