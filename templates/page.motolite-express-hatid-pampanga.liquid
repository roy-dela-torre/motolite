            { min: 1199, size: 2 },
            { min: 768, size: 2 },
            { min: 0, size: 1 }
        ]);
    }

    $(document).ready(function () {
        applyEqualHeights();

        // Remove empty <p> and <br>
        $('p').each(function () {
            if ($(this).text().trim() === '') {
                $(this).remove();
            }
        });
        $("br").remove();

        // Re-apply equal heights every 1000ms
        setInterval(function () {
            applyEqualHeights();
        }, 1000);
    });

</script>