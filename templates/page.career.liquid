    /*Large*/
    function FindJobsLarge() {
          let container = document.getElementById("career-search-result-container");
      
          let searchTitleInputLarge = document.querySelector(".job-title-input-large");
          let searchLocationInputLarge = document.querySelector(".location-input-large");  
          let datePosted = document.getElementById("datePostedButton");
          let jobType = document.getElementById("jobTypeDropdownButton");
          let workArrangement = document.getElementById("workArrangementDropdownButton");
            
          /*fetch Api*/
          document.querySelector(".text-title").classList.remove('hide');
          document.querySelector("#app-process").classList.add("hide");
          document.querySelector("#form-container").classList.add("hide");
          document.querySelector("#dropDownContainer").classList.remove('hide');
          document.querySelector("#backButton").classList.remove('hide');
          document.getElementById("page").classList.remove('hide');
          
        
          if(container.hasChildNodes())
              container.replaceChildren();

          let imagesContainer = document.getElementById("images-container");
          let carouselContainer = document.getElementById("carouselExampleCaptions");
        
          if(imagesContainer != null && imagesContainer.hasChildNodes())
          {
            imagesContainer.parentNode.removeChild(imagesContainer);
            carouselContainer.parentNode.removeChild(carouselContainer);
          }
        
          let jobTypeInnerText = jobType.innerText == 'Job Type' ? '' : jobType.innerText;
          let workArrangementInnerText = workArrangement.innerText == 'Work Arrangement' ? '' : workArrangement.innerText;

          let datePostedInnerText = datePosted.innerText.toUpperCase();
          let careerPagesDateRange = careerPages;
          if(datePostedInnerText != 'DATE POSTED' && datePostedInnerText != 'ALL-TIME')
          {
            let startDate = new Date();
            let endDate = new Date();
            if(datePostedInnerText == "PAST WEEK")
